    "hoc",
    "higher order component",
    "validate"
  ],
  "contributor": [
    {
      "name": "Lee",
      "email": "jessy1092@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jessy1092/valle"
